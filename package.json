{
  "name": "sebu",
  "version": "0.0.3",
  "description": "SEarch files and BUmp up version",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "node -r esbuild-register src/index.ts",
    "prebuild": "rimraf dist",
    "build": "run-p build:transpile build:declaration",
    "build:transpile": "node -r esbuild-register scripts/build.ts",
    "build:declaration": "tsc --emitDeclarationOnly",
    "release": "shipjs prepare"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tyankatsu0105/sebu.git"
  },
  "keywords": [
    "bump",
    "search"
  ],
  "author": "tyankatsu <frips.ryilsufupe+dev@gmail.com> (https://tyankatsu.netlify.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tyankatsu0105/sebu/issues"
  },
  "homepage": "https://github.com/tyankatsu0105/sebu#readme",
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/node": "12.20.3",
    "esbuild": "0.12.4",
    "esbuild-register": "2.5.0",
    "glob": "7.1.7",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "shipjs": "0.23.2",
    "typescript": "4.2.4"
  }
}
