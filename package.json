    "search"
  ],
  "author": "tyankatsu <frips.ryilsufupe+dev@gmail.com> (https://tyankatsu.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tyankatsu0105/sebu/issues"
  },
  "homepage": "https://github.com/tyankatsu0105/sebu#readme",
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/node": "12.20.3",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "commitizen": "4.2.4",
    "cz-format-extension": "1.4.0",
    "esbuild": "0.12.4",
    "esbuild-register": "2.5.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-destructure-keys": "1.3.5",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-typescript-sort-keys": "1.6.0",
    "lint-staged": "11.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "shipjs": "0.23.2",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "commander": "^7.2.0",
    "glob": "^7.1.7"
  }
